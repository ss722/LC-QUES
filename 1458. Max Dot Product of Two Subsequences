class Solution {
public:
    int n,m;
    int f(int i,int j,vector<int>& nums1, vector<int>& nums2,vector<vector<int>> &dp){
        if(i==n || j==m) return 0;
        if(dp[i][j]!=-1)return dp[i][j];

        return dp[i][j] =  max({
        nums1[i]*nums2[j] + f(i+1,j+1,nums1,nums2,dp),
        f(i+1,j,nums1,nums2,dp),
        f(i,j+1,nums1,nums2,dp)});
    }
    int maxDotProduct(vector<int>& nums1, vector<int>& nums2) {
        n=nums1.size();
        m=nums2.size();
        vector<vector<int>> dp(n+1,vector<int>(m+1,0));
        int maxi = INT_MIN;
        for(int i=n-1;i>=0;i--){
            for(int j=m-1;j>=0;j--){
                 maxi = max(maxi,nums1[i]*nums2[j]); 
                dp[i][j] =  max({
                nums1[i]*nums2[j] + dp[i+1][j+1],
                dp[i+1][j], dp[i][j+1] });
            }
        }

        int ans = dp[0][0];
        if(ans>0) return ans;
        return maxi;
    }
};
