class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        int last = 0;
        int i=0;
        while(i<bits.size()){
            if(bits[i]==0){
                last = i;
                i++;
            }
            else{
                last = i;
                i+=2;
            }
        }
        if(last==bits.size()-1) return true;
        return false;

    }
};
