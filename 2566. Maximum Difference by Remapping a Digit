class Solution {
public:
    int minMaxDifference(int num) {
        
        string  s1 = to_string(num);
        string s2=s1;
        auto pos = s1.find_first_not_of('9');
        if(pos!=string::npos){
            char c = s1[pos];
            replace(s1.begin(),s1.end(),c,'9');
        }
        char c = s2[0];
        replace(s2.begin(),s2.end(),c,'0');
        return stoi(s1)-stoi(s2); 
    }
};
