class Solution {
public:
    vector<string> removeAnagrams(vector<string>& words) {
        vector<string> v;
        string last = "";
        for(int i=0;i<words.size();i++){
            string curr = words[i];
            sort(curr.begin(),curr.end());
            if(curr==last) continue;
            last = curr;
            v.push_back(words[i]);
        }
        return v;
    }
};
