class Solution {
public:
    long long zeroFilledSubarray(vector<int>& nums) {
        int curr  = 0;
        long long ans = 0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==0){
                curr++;
                ans+= curr;
            }
            else curr=0;
        }
        return ans;
    }
};
/*
2->3
3->6

n*(n+1)/2

*/
